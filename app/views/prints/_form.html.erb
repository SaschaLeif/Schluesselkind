<%= form_for(@print) do |f| %>
<% if @print.errors.any? %>
<div id="error_explanation">
	<h2><%= pluralize(@print.errors.count, "error") %> prohibited this print from being saved:</h2>

	<ul>
		<% @print.errors.full_messages.each do |msg| %>
